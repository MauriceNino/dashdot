    "react-is": "18.2.0",
    "react-spinners-kit": "1.9.1",
    "react-virtualized-auto-sizer": "1.0.25",
    "recharts": "2.7.3",
    "regenerator-runtime": "0.14.1",
    "rxjs": "7.8.2",
    "sass": "1.63.6",
    "simplebar-react": "3.2.4",
    "socket.io": "4.7.4",
    "socket.io-client": "4.7.4",
    "store": "2.0.12",
    "styled-components": "6.1.16",
    "systeminformation": "5.23.8",
    "throttle-debounce": "5.0.2",
    "tslib": "2.6.2",
    "use-color-scheme": "1.1.3",
    "util": "0.12.5",
    "web-vitals": "3.3.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@commitlint/cz-commitlint": "18.4.4",
    "@docusaurus/module-type-aliases": "3.1.0",
    "@nx-plus/docusaurus": "15.0.0-rc.0",
    "@nx/cypress": "17.2.8",
    "@nx/esbuild": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/express": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/webpack": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@semantic-release-plus/docker": "3.1.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@testing-library/react": "14.3.1",
    "@types/chai": "4.3.11",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.25",
    "@types/node-cron": "3.0.11",
    "@types/qs": "6.9.11",
    "@types/react": "18.2.48",
    "@types/react-color": "3.0.13",
    "@types/react-dom": "18.2.18",
    "@types/react-is": "18.2.4",
    "@types/react-virtualized-auto-sizer": "1.0.4",
    "@types/store": "2.0.5",
    "@types/styled-components": "5.1.34",
    "@types/throttle-debounce": "5.0.2",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-c8": "0.32.4",
    "@vitest/ui": "0.32.4",
    "babel-jest": "29.7.0",
    "babel-plugin-styled-components": "2.1.4",
    "babel-plugin-syntax-jsx": "6.18.0",
    "chai": "4.4.1",
    "commitizen": "4.3.1",
    "dotenv": "16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "inquirer": "8.2.6",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jsdom": "22.1.0",
    "nx": "17.2.8",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "semantic-release-plus": "20.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "vite": "5.4.14",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "0.32.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ],
      "footer-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "avoid",
    "jsxSingleQuote": true
  },
  "release": {
    "branches": [
      "main",
      "dev"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": ".github/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": ":tada: This issue has been resolved in version ${nextRelease.version}\n\nPlease check the [changelog](https://github.com/MauriceNino/dashdot/blob/main/.github/CHANGELOG.md) for more details.",
          "successCommentCondition": "<% return issue.user.type !== 'Bot'; %>",
          "failComment": false,
          "failTitle": false,
          "labels": false,
          "releasedLabels": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            ".github/CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [CI SKIP]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "packageManager": "yarn@3.8.7"
}
