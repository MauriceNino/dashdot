    "@fortawesome/react-fontawesome": "0.2.2",
    "@mantine/core": "6.0.22",
    "@mantine/hooks": "6.0.22",
    "@mdx-js/react": "3.1.0",
    "@swc/helpers": "0.5.3",
    "antd": "5.13.2",
    "axios": "1.8.4",
    "clsx": "1.2.1",
    "compression": "1.7.4",
    "core-js": "3.35.0",
    "cors": "2.8.5",
    "cronstrue": "2.56.0",
    "dedent": "1.5.3",
    "docusaurus-plugin-image-zoom": "1.0.1",
    "docusaurus-plugin-sass": "0.2.6",
    "express": "4.20.0",
    "framer-motion": "10.18.0",
    "node-cron": "3.0.3",
    "parse-domain": "7.0.1",
    "prism-react-renderer": "2.3.1",
