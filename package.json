{
  "name": "dashdot",
  "version": "0.0.0",
  "description": "dash. - a modern server dashboard",
  "license": "MIT",
  "private": true,
  "main": "dist/apps/api/main.js",
  "repository": "https://github.com/MauriceNino/dashdot",
  "homepage": "https://getdashdot.com",
  "author": "MauriceNino <mauriceprivat98@gmail.com>",
  "bugs": {
    "url": "https://github.com/MauriceNino/dashdot/issues"
  },
  "keywords": [
    "monitoring",
    "dashboard",
    "server",
    "glassmorphism"
  ],
  "scripts": {
    "start": "node .",
    "cli": "node dist/apps/cli/main.js",
    "build": "nx run-many --target=build --configuration=production --all=true --parallel=true",
    "build:prod": "nx run-many --target=build --configuration=production --projects=api,cli,view --parallel=true --verbose",
    "serve": "nx run-many --target=serve --all=true --parallel=true",
    "dev": "docker compose -p dashdot_dev -f ./docker-compose.yml up --remove-orphans",
    "test": "nx run-many --target=test --all=true --parallel=true",
    "postinstall": "husky install",
    "commit": "git-cz",
    "clean": "rm -rf dist apps/docs/.docusaurus node_modules/.cache"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@easyops-cn/docusaurus-search-local": "0.35.0",
    "@emotion/memoize": "0.8.0",
    "@emotion/react": "11.10.6",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-brands-svg-icons": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mantine/core": "6.0.5",
    "@mantine/hooks": "6.0.5",
    "@mdx-js/react": "1.6.22",
    "antd": "5.3.3",
    "axios": "1.3.4",
    "clsx": "1.2.1",
    "compression": "1.7.4",
    "core-js": "3.29.1",
    "cors": "2.8.5",
    "docusaurus-plugin-image-zoom": "0.1.1",
    "docusaurus-plugin-sass": "0.2.3",
    "express": "4.18.2",
    "framer-motion": "10.9.2",
    "parse-domain": "7.0.1",
    "prism-react-renderer": "1.3.5",
    "punycode": "2.3.0",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-form-ctl": "2.0.1",
    "react-is": "18.2.0",
    "react-spinners-kit": "1.9.1",
    "react-virtualized-auto-sizer": "1.0.7",
    "recharts": "2.5.0",
    "regenerator-runtime": "0.13.11",
    "rxjs": "7.8.0",
    "sass": "1.60.0",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1",
    "store": "2.0.12",
    "styled-components": "5.3.9",
    "systeminformation": "5.17.12",
    "throttle-debounce": "5.0.0",
    "tslib": "2.5.0",
    "use-color-scheme": "1.1.3",
    "util": "0.12.5",
    "web-vitals": "3.3.0",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/cz-commitlint": "17.5.0",
    "@docusaurus/module-type-aliases": "2.4.0",
    "@nrwl/cli": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/express": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/node": "14.7.5",
    "@nrwl/react": "14.7.5",
    "@nrwl/tao": "14.7.5",
    "@nrwl/web": "14.7.5",
    "@nrwl/workspace": "14.7.5",
    "@nx-plus/docusaurus": "14.1.0",
    "@semantic-release-plus/docker": "3.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@testing-library/react": "14.0.0",
    "@types/chai": "4.3.4",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.31",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.11",
    "@types/react-is": "17.0.3",
    "@types/react-virtualized-auto-sizer": "1.0.1",
    "@types/store": "2.0.2",
    "@types/styled-components": "5.1.26",
    "@types/throttle-debounce": "5.0.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "babel-jest": "29.5.0",
    "babel-plugin-styled-components": "2.0.7",
    "babel-plugin-syntax-jsx": "6.18.0",
    "chai": "4.3.7",
    "commitizen": "4.3.0",
    "dotenv": "16.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "inquirer": "8.2.4",
    "jest": "29.5.0",
    "nx": "14.7.5",
    "prettier": "2.8.7",
    "prop-types": "15.8.1",
    "react-test-renderer": "18.2.0",
    "semantic-release-plus": "20.0.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ],
      "footer-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "avoid",
    "jsxSingleQuote": true
  },
  "release": {
    "branches": [
      "main",
      "dev"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": ".github/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": ":tada: This issue has been resolved in version ${nextRelease.version}\n\nPlease check the [changelog](https://github.com/MauriceNino/dashdot/blob/main/.github/CHANGELOG.md) for more details.",
          "failComment": false,
          "failTitle": false,
          "labels": false,
          "releasedLabels": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            ".github/CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [CI SKIP]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "packageManager": "yarn@3.5.0"
}
